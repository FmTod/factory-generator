{
    "name": "laravel-shift/factory-generator",
    "description": "Generate factories from existing models",
    "type": "package",
    "license": "MIT",
    "keywords": ["laravel", "factory", "model", "testing"],
    "require": {
        "illuminate/support": "^8.0",
        "fzaninotto/faker": "^1.9.1",
        "naoray/eloquent-model-analyzer": "^2.0",
        "nikic/php-parser": "^4.10"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Shift\\FactoryGenerator\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shift\\FactoryGenerator\\Tests\\": "tests",
            "App\\": "./vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shift\\FactoryGenerator\\FactoryGeneratorServiceProvider"
            ]
        }
    }
}
